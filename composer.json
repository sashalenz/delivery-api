{
    "name": "sashalenz/delivery-api",
    "description": "Delivery API",
    "keywords": [
        "sashalenz",
        "delivery-api"
    ],
    "homepage": "https://github.com/sashalenz/delivery-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Petrovskyi",
            "email": "sashalenz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "nesbot/carbon": "^2.57",
        "spatie/laravel-package-tools": "^1.9",
        "spatie/data-transfer-object": "^3.7",
        "illuminate/contracts": "^8.0|^9.0|^9.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Sashalenz\\Delivery\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sashalenz\\Delivery\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sashalenz\\Delivery\\DeliveryServiceProvider"
            ],
            "aliases": {
                "Delivery": "Sashalenz\\Delivery\\DeliveryFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
