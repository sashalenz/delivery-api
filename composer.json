{
    "name": "sashalenz/delivery-api",
    "description": "Delivery API",
    "keywords": [
        "sashalenz",
        "delivery-api"
    ],
    "homepage": "https://github.com/sashalenz/delivery-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Petrovskyi",
            "email": "sashalenz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "nesbot/carbon": "^2.46",
        "spatie/laravel-package-tools": "^1.6",
        "spatie/data-transfer-object": "^3.7",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.16",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Sashalenz\\Delivery\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sashalenz\\Delivery\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sashalenz\\Delivery\\DeliveryServiceProvider"
            ],
            "aliases": {
                "Delivery": "Sashalenz\\Delivery\\DeliveryFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
